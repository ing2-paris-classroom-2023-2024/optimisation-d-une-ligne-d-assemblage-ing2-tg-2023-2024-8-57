typedef struct {
    float totalTime;
    int* vertices;
    int numVertices;
} Group;
// Structure pour un sommet
typedef struct Node {
    int vertex;
    float time;
    struct Node* next;
} Node;

// Structure pour la liste d'adjacence
typedef struct AdjList {
    Node* head;
} AdjList;

typedef struct Graph {
    int numVertices;
    AdjList* array;
    float* times; // Ajout d'un tableau pour stocker les temps
} Graph;
